{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "5.3.0",
    "generators": {
      "axios": {
        "generatorName": "typescript-axios",
        "output": "#{cwd}/output/axios",
        "glob": "schemas/top-api.yml",
        "typeMappings": "DateTime=Date",
        "additionalProperties": {
          "npmName": "@onto-med/top-api",
          "npmRepository": "https://npm.pkg.github.com",
          "supportsES6": "true",
          "withInterfaces": true,
          "withoutPrefixEnums": true
        }
      }
    }
  }
}
